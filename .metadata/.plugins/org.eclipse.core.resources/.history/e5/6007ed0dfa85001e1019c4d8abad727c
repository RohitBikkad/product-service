package com.productservice.dto;

public class ProductVariantDTO {

    private Long id;
    private String sku;
    
    
    
    
	public ProductVariantDTO() {
		super();
	}
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getSku() {
		return sku;
	}
	public void setSku(String sku) {
		this.sku = sku;
	}

    
}
